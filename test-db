import time
import pymongo
from pymongo import MongoClient

client = pymongo.MongoClient("mongodb://localhost:27017/")

# Database Name
db = client["piiclone"]
 
# Collection Name
col = db["users"]
 
x = col.find({},{'_id': 1,})
 
for data in x:
    print(data)

